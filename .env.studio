# ArcticCodex Studio - Environment Configuration

# LLM Endpoint (if using remote LLM)
# AC_LLM_ENDPOINT=http://localhost:8000

# Embeddings
AC_EMBEDDINGS=1
AC_EMBEDDINGS_MODEL=sentence-transformers/all-MiniLM-L6-v2

# Vault path
VAULT_PATH=./vault

# Port for studio server
PORT=8000

# Development mode
PYTHON_ENV=development

# Optional: Audit logging
AUDIT_COMPLIANCE_LEVEL=standard
AUDIT_STORAGE_DIR=./audit_logs
